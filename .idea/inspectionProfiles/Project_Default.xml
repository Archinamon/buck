<component name="InspectionProjectProfileManager">
  <profile version="1.0">
    <option name="myName" value="Project Default" />
    <inspection_tool class="SSBasedInspection" enabled="true" level="ERROR" enabled_by_default="true">
      <searchConfiguration name="Android direct text usage is prohibited — use StringProxy class instead" text="&lt;$tag$ $attribute$=&quot;$value$&quot;/&gt;" recursive="true" caseInsensitive="true" type="XML">
        <constraint name="__context__" within="" contains="" />
        <constraint name="tag" within="" contains="" />
        <constraint name="attribute" regexp="android:text" wholeWordsOnly="true" within="" contains="" />
        <constraint name="value" minCount="0" maxCount="0" target="true" within="" contains="" />
      </searchConfiguration>
      <searchConfiguration name="Direct usage of R.string is not allowed — use StringProxy class instead" text="$className$.$fieldName$" recursive="true" caseInsensitive="true" type="JAVA">
        <constraint name="__context__" within="" contains="" />
        <constraint name="className" regexp="R.string" withinHierarchy="true" wholeWordsOnly="true" within="" contains="" />
        <constraint name="fieldName" within="" contains="" />
      </searchConfiguration>
    </inspection_tool>
  </profile>
</component>